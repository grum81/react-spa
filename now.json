{
  "name": "react-spa-example",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "*.css",
      "use": "@now/static"
    },
    {
      "src": "*.js",
      "use": "@now/static"
    },
    {
      "src": "*.html",
      "use": "@now/static"
    }
  ]
}
